<?xml version="1.0" encoding="utf-8" ?>
<ApplicationConfig>
  <LoginParameters>
    <StationNumber>XOFL01-002-01</StationNumber>
    <Client>01</Client>
    <RegistrationType>S</RegistrationType>
    <RefreshTimeSpan>10</RefreshTimeSpan> 
    <!--unit is 's'-->
    <ClearConsoleTimeSpan>1</ClearConsoleTimeSpan>
    <!--unit is 'Hr'-->
    <DBType>ORACLE</DBType>
    <AUTH_TEAM></AUTH_TEAM>

    <!--真实区-->
    <!--<IPAddress>172.17.1.5</IPAddress>
    <Port>50550</Port>-->

