    <SyncICPartToiTAC>Enable</SyncICPartToiTAC>
    <TRANSFER_AUTO_STOCK_IN>0E10;0X10</TRANSFER_AUTO_STOCK_IN>
    <CGS_DB_SERVER>uid;pwd;dsn</CGS_DB_SERVER>
    <CGS_DB_SERVER_OLEDB>CGS;172.17.1.5:50000;cgsapp;T0mcat4Fun</CGS_DB_SERVER_OLEDB>
